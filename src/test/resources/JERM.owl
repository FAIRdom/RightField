        <dc:identifier>JERM:50015</dc:identifier>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#ExperimentDescription -->

    <owl:Class rdf:about="#ExperimentDescription">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#title"/>
