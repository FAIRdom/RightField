    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#Comparative_genomic_hybridization -->

    <owl:Class rdf:about="#Comparative_genomic_hybridization">
        <rdfs:subClassOf rdf:resource="#transcripomics"/>
        <dc:identifier>JERM:00086</dc:identifier>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#CultureGrowth -->

    <owl:Class rdf:about="#CultureGrowth">
        <rdfs:subClassOf rdf:resource="#TechnologyType"/>
        <dc:description
            >SysMO studies microorganisms. Each experiment is performed on cell cultures, so it is essential to record the type of culture in use each time</dc:description>
        <dc:identifier>JERM:00026</dc:identifier>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#DNA_Methylation -->

    <owl:Class rdf:about="#DNA_Methylation">
        <rdfs:subClassOf rdf:resource="#microarray"/>
        <dc:identifier>JERM:00131</dc:identifier>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#DNA_sequencing -->

    <owl:Class rdf:about="#DNA_sequencing">
        <rdfs:subClassOf rdf:resource="#genomics"/>
        <dc:identifier>JERM:00058</dc:identifier>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#Data -->

    <owl:Class rdf:about="#Data">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasFormat"/>
                <owl:someValuesFrom rdf:resource="#DataFileFormat"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#Asset"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasPart"/>
                <owl:someValuesFrom rdf:resource="#FactorsStudied"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:identifier>JERM:00011</dc:identifier>
        <dc:description>SEEK data files</dc:description>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#DataFileFormat -->

    <owl:Class rdf:about="#DataFileFormat">
        <rdfs:subClassOf rdf:resource="#Format"/>
        <owl:disjointWith rdf:resource="#ModelExecutionEnvironment"/>
        <owl:disjointWith rdf:resource="#ModelFormat"/>
        <owl:disjointWith rdf:resource="#SOPFileFormat"/>
        <dc:identifier>JERM:00038</dc:identifier>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#Detectors -->

    <owl:Class rdf:about="#Detectors">
        <rdfs:subClassOf rdf:resource="#PostSourceComponent"/>
