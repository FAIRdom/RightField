<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY JERMOntology "http://www.mygrid.org.uk/ontology/JERMOntology#" >
    <!ENTITY JERMOntology3 "http://www.mygrid.org.uk/ontology/JERMOntology#2" >
    <!ENTITY JERMOntology2 "http://www.mygrid.org.uk/ontology/JERMOntology#13" >
    <!ENTITY JERMOntology4 "http://www.mygrid.org.uk/ontology/JERMOntology#2-" >
]>


<rdf:RDF xmlns="http://www.mygrid.org.uk/ontology/JERMOntology#"
     xml:base="http://www.mygrid.org.uk/ontology/JERMOntology"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:JERMOntology3="&JERMOntology;2"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:JERMOntology2="&JERMOntology;13"
     xmlns:JERMOntology="http://www.mygrid.org.uk/ontology/JERMOntology#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:JERMOntology4="&JERMOntology;2-">
    <owl:Ontology rdf:about=""/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&dc;description"/>
    <owl:AnnotationProperty rdf:about="&dc;identifier"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#hasAssociatedItem -->

    <owl:ObjectProperty rdf:about="#hasAssociatedItem">
        <rdfs:domain rdf:resource="#Project"/>
        <owl:inverseOf rdf:resource="#isAssociatedWith"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Asset"/>
                    <rdf:Description rdf:about="#ExperimentDescription"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#hasContributor -->

    <owl:ObjectProperty rdf:about="#hasContributor">
        <rdfs:range rdf:resource="#Person"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Asset"/>
                    <rdf:Description rdf:about="#ExperimentDescription"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#hasFormat -->

    <owl:ObjectProperty rdf:about="#hasFormat">
        <rdfs:domain rdf:resource="#Asset"/>
        <rdfs:range rdf:resource="#Format"/>
        <owl:inverseOf rdf:resource="#isFormatOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#hasPart -->

    <owl:ObjectProperty rdf:about="#hasPart">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <owl:inverseOf rdf:resource="#isPartOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#hasType -->

    <owl:ObjectProperty rdf:about="#hasType">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:domain rdf:resource="#Assay"/>
        <owl:inverseOf rdf:resource="#isTypeOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#investigates -->

    <owl:ObjectProperty rdf:about="#investigates">
        <rdfs:range rdf:resource="#organism"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Assay"/>
                    <rdf:Description rdf:about="#Project"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#isAssociatedWith -->

    <owl:ObjectProperty rdf:about="#isAssociatedWith">
        <rdfs:range rdf:resource="#Project"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Asset"/>
                    <rdf:Description rdf:about="#ExperimentDescription"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#isContributorOf -->

    <owl:ObjectProperty rdf:about="#isContributorOf">
        <rdfs:domain rdf:resource="#Person"/>
        <owl:inverseOf rdf:resource="#hasContributor"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Asset"/>
                    <rdf:Description rdf:about="#ExperimentDescription"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#isDerivedFrom -->

    <owl:ObjectProperty rdf:about="#isDerivedFrom">
        <rdfs:domain rdf:resource="#Model"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Data"/>
                    <rdf:Description rdf:about="#Publication"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#isFormatOf -->

    <owl:ObjectProperty rdf:about="#isFormatOf">
        <rdfs:range rdf:resource="#Asset"/>
        <rdfs:domain rdf:resource="#Format"/>
    </owl:ObjectProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#isPartOf -->

    <owl:ObjectProperty rdf:about="#isPartOf">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#isTypeOf -->

    <owl:ObjectProperty rdf:about="#isTypeOf">
        <rdfs:range rdf:resource="#Assay"/>
    </owl:ObjectProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#is_investigated_by -->

    <owl:ObjectProperty rdf:about="#is_investigated_by">
        <owl:inverseOf rdf:resource="#investigates"/>
        <rdfs:domain rdf:resource="#organism"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Assay"/>
                    <rdf:Description rdf:about="#Project"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#technologyUsedIn -->

    <owl:ObjectProperty rdf:about="#technologyUsedIn">
        <rdfs:range rdf:resource="#AssayType"/>
        <rdfs:domain rdf:resource="#TechnologyType"/>
        <owl:inverseOf rdf:resource="#usesTechnology"/>
    </owl:ObjectProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#usesTechnology -->

    <owl:ObjectProperty rdf:about="#usesTechnology">
        <rdfs:domain rdf:resource="#AssayType"/>
        <rdfs:range rdf:resource="#TechnologyType"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#ECNumber -->

    <owl:DatatypeProperty rdf:about="#ECNumber"/>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#NCBI_ID -->

    <owl:DatatypeProperty rdf:about="#NCBI_ID"/>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#WebLink -->

    <owl:DatatypeProperty rdf:about="#WebLink"/>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#description -->

    <owl:DatatypeProperty rdf:about="#description"/>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#genotype -->

    <owl:DatatypeProperty rdf:about="#genotype"/>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#instrument_model -->

    <owl:DatatypeProperty rdf:about="#instrument_model">
        <rdfs:label
            >instrument_model</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#manufacturer -->

    <owl:DatatypeProperty rdf:about="#manufacturer">
        <rdfs:label>manufacturer</rdfs:label>
        <rdfs:domain rdf:resource="#Instrument"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#phenotype -->

    <owl:DatatypeProperty rdf:about="#phenotype"/>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#reactionCatalyst -->

    <owl:DatatypeProperty rdf:about="#reactionCatalyst"/>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#sequence_accession -->

    <owl:DatatypeProperty rdf:about="#sequence_accession">
        <rdfs:label
            >sequence_accession</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#strain -->

    <owl:DatatypeProperty rdf:about="#strain"/>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#termsofDistribution -->

    <owl:DatatypeProperty rdf:about="#termsofDistribution">
        <rdfs:label
            >termsofDistribution</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#timepoint -->

    <owl:DatatypeProperty rdf:about="#timepoint">
        <rdfs:label>timepoint</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#title -->

    <owl:DatatypeProperty rdf:about="#title"/>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#units -->

    <owl:DatatypeProperty rdf:about="#units">
        <rdfs:label>units</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#version -->

    <owl:DatatypeProperty rdf:about="#version">
        <rdfs:label>version</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#13C_radiolabelling -->

    <owl:Class rdf:about="#13C_radiolabelling">
        <rdfs:subClassOf rdf:resource="#radioactive_labelling"/>
        <dc:identifier>JERM:00137</dc:identifier>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#2-hybrid_system -->

    <owl:Class rdf:about="#2-hybrid_system">
        <rdfs:subClassOf rdf:resource="#binding"/>
        <rdfs:comment>yeast 2 hybrid</rdfs:comment>
        <dc:identifier>JERM:00106</dc:identifier>
        <dc:description
            >Two-hybrid screening (also known as yeast two-hybrid system or Y2H) is a molecular biology technique used to discover protein-protein interactions and protein-DNA interactions by testing for physical interactions</dc:description>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#2D_gel_electrophoresis -->

    <owl:Class rdf:about="#2D_gel_electrophoresis">
        <rdfs:subClassOf rdf:resource="#gel_electrophoresis"/>
        <dc:identifier>JERM:00115</dc:identifier>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#Accelerator_Mass_Spectrometry -->

    <owl:Class rdf:about="#Accelerator_Mass_Spectrometry">
        <rdfs:subClassOf rdf:resource="#mass_Spectrometry"/>
        <dc:identifier>JERM:00120</dc:identifier>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#Algebraic_equation -->

    <owl:Class rdf:about="#Algebraic_equation">
        <rdfs:subClassOf rdf:resource="#mathematical_modelType"/>
        <dc:identifier>JERM:00139</dc:identifier>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#Assay -->

    <owl:Class rdf:about="#Assay">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasPart"/>
                        <owl:someValuesFrom rdf:resource="#Asset"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasType"/>
                        <owl:someValuesFrom rdf:resource="#AssayType"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasType"/>
                        <owl:someValuesFrom rdf:resource="#TechnologyType"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#isPartOf"/>
                        <owl:someValuesFrom rdf:resource="#Study"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#ExperimentDescription"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasType"/>
                <owl:someValuesFrom rdf:resource="#CultureGrowth"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#investigates"/>
                <owl:someValuesFrom rdf:resource="#organism"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#Investigation"/>
        <owl:disjointWith rdf:resource="#Study"/>
        <dc:identifier>JERM:00021</dc:identifier>
        <dc:description
            >descriptions of individual experiments</dc:description>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#AssayType -->

    <owl:Class rdf:about="#AssayType">
        <rdfs:subClassOf rdf:resource="#ExperimentalFactors"/>
        <owl:disjointWith rdf:resource="#TechnologyType"/>
        <rdfs:comment
            >The assay_type hierarchy allows scientists to categorise their experimental assays, informatics analyses and computational models within SysMO</rdfs:comment>
        <dc:identifier>JERM:00024</dc:identifier>
        <dc:description
            >An assay type describes the type of experiment being performed</dc:description>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#Asset -->

    <owl:Class rdf:about="#Asset">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasContributor"/>
                        <owl:someValuesFrom rdf:resource="#Person"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#isAssociatedWith"/>
                        <owl:someValuesFrom rdf:resource="#Project"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#isPartOf"/>
                        <owl:someValuesFrom rdf:resource="#Assay"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#description"/>
                        <owl:someValuesFrom rdf:resource="&xsd;string"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#title"/>
                        <owl:someValuesFrom rdf:resource="&xsd;string"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="#Equipment"/>
        <owl:disjointWith rdf:resource="#ExperimentDescription"/>
        <owl:disjointWith rdf:resource="#ExperimentalFactors"/>
        <owl:disjointWith rdf:resource="#Format"/>
        <owl:disjointWith rdf:resource="#Project"/>
        <dc:description
            >all shareable entities in the SysMO SEEK are kinds of Assets</dc:description>
        <dc:identifier>JERM:00010</dc:identifier>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#Batch -->

    <owl:Class rdf:about="#Batch">
        <rdfs:subClassOf rdf:resource="#CultureGrowth"/>
        <owl:disjointWith rdf:resource="#Chemostat"/>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#BioCharacteristics -->

    <owl:Class rdf:about="#BioCharacteristics">
        <rdfs:subClassOf rdf:resource="#MicroarrayFactors"/>
        <dc:identifier>JERM:22481</dc:identifier>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#BioMaterial -->

    <owl:Class rdf:about="#BioMaterial">
        <rdfs:subClassOf rdf:resource="#ExperimentalFactors"/>
        <rdfs:comment
            >This part of the ontology is still in development</rdfs:comment>
        <dc:identifier>JERM:67650</dc:identifier>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#Buffer -->

    <owl:Class rdf:about="#Buffer">
        <rdfs:subClassOf rdf:resource="#FactorsStudied"/>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#CellML -->

    <owl:Class rdf:about="#CellML">
        <rdfs:subClassOf rdf:resource="#ModelFormat"/>
        <dc:identifier>JERM:40020</dc:identifier>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#ChIP-on-chip -->

    <owl:Class rdf:about="#ChIP-on-chip">
        <rdfs:subClassOf rdf:resource="#microarray"/>
        <rdfs:comment
            >Chromatin Immunoprecipitation on Chip</rdfs:comment>
        <dc:identifier>JERM:00130</dc:identifier>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#Chemostat -->

    <owl:Class rdf:about="#Chemostat">
        <rdfs:subClassOf rdf:resource="#CultureGrowth"/>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#CollisionCell -->

    <owl:Class rdf:about="#CollisionCell">
        <rdfs:subClassOf rdf:resource="#PostSourceComponent"/>
