    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#Equipment -->

    <owl:Class rdf:about="#Equipment">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <owl:disjointWith rdf:resource="#ExperimentDescription"/>
        <owl:disjointWith rdf:resource="#ExperimentalFactors"/>
        <owl:disjointWith rdf:resource="#Format"/>
        <owl:disjointWith rdf:resource="#Person"/>
        <owl:disjointWith rdf:resource="#Project"/>
        <dc:identifier>JERM:90004</dc:identifier>
