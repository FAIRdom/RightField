    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#EnzymeReactionAssay -->

    <owl:Class rdf:about="#EnzymeReactionAssay">
        <rdfs:subClassOf rdf:resource="#Assay"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasType"/>
                <owl:someValuesFrom rdf:resource="#enzymaticAssay"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:identifier>JERM:40006</dc:identifier>
    </owl:Class>
    


    <!-- http://www.mygrid.org.uk/ontology/JERMOntology#EnzymeReactionData -->

    <owl:Class rdf:about="#EnzymeReactionData">
        <rdfs:subClassOf rdf:resource="#Data"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#sequence_accession"/>
